{
	"compileOnSave": true,
	"compilerOptions": {
		"module": "commonjs",
		"sourceMap": true,
		"declaration": true,
		"watch": false,
		"outDir": ".",
		"noEmitOnError": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"types": [
			"node",
			"jest",
			"jest-expect-message"
		],
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"target": "ES2020",
		"lib": [
			"ES2020"
		]
	},
	"include": [
		"_src"
	],
	"exclude": [
		"testservices"
	]
}
